<Window
    CanResize="False"
    Height="500"
    Icon="/Assets/avalonia-logo.ico"
    MaxHeight="500"
    MaxWidth="300"
    Title="CampusPay"
    Width="380"
    WindowStartupLocation="CenterScreen"
    d:DesignHeight="500"
    d:DesignWidth="300"
    mc:Ignorable="d"
    x:Class="ClientApp.Views.AuthWindow"
    x:DataType="vm:AuthWindowViewModel"
    xmlns="https://github.com/avaloniaui"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="using:FluentAvalonia.UI.Controls"
    xmlns:views="using:ClientApp.Views"
    xmlns:vm="using:ClientApp.ViewModels"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Design.DataContext>
        <vm:AuthWindowViewModel />
    </Design.DataContext>
    <Border BorderBrush="{DynamicResource ControlStrokeColorDefaultBrush}" BorderThickness="1">
        <ui:Frame
            MaxHeight="500"
            MaxWidth="500"
            MinHeight="250"
            MinWidth="250"
            Name="AuthFrame" />
    </Border>

</Window>